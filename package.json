  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run build && node dist/index.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production babel server/index.js --out-dir dist",
    "build:client": "npm run clean && webpack --config ./webpack/webpack.prod.js",
    "develop:server": "nodemon --exec babel-node server/index.js",
    "develop:client": "webpack-dev-server --config ./webpack/webpack.dev.js"
  },
