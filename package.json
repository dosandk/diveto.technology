    "copy-webpack-plugin": "^5.0.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
